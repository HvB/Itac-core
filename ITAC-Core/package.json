{
  "name": "ItacExpress",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www | bunyan -l debug",
    "test": "mocha --recursive --reporter=xunit --reporter-options output=./test-reports/junit1.xml",
    "coverage": "nyc --reporter=lcov --reporter=text-summary  mocha --recursive --reporter mocha-sonarqube-reporter --reporter-options output=./test-reports/unit.xml",
    "sonar-scanner": "sonar-scanner -Dsonar.tests=. -Dsonar.test.inclusions=test/** -Dsonar.sources=. -Dsonar.exclusions=test/**,**/lib/**,**/coverage/**,**/build/** -Dsonar.sourceEncoding=UTF-8 -Dsonar.gitlab.project_id=ITAC-Core-GPL -Dsonar.testExecutionReportPaths=test-reports/unit.xml -Dsonar.javascript.lcov.reportPaths=coverage/lcov.info  "
  },
  "devDependencies": {
    "chai": ">= 4",
    "chai-as-promised": "*",
    "mocha": "*",
    "mocha-sonarqube-reporter": "^1.0.1",
    "nyc": "^11.4.1",
    "sonar-scanner": "^3.0.3"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "ejs": "*",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0",
    "socket.io": ">= 1.6.0",
    "express-cors": "*",
    "jquery": "1.12.4",
    "jquery-ui": "1.12.0-rc.2",
    "merge": "*",
    "nodemailer": "~2.3",
    "ldapjs": "*",
    "bunyan": "*",
    "mkdirp": "*",
    "uuid": "*",
    "fast-json-patch": "*"
  }
}
